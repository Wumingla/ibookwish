<%= javascript_include_tag "app" %>
<%= stylesheet_link_tag    "application", "http://ajax.googleapis.com/ajax/libs/jqueryui/1/themes/flick/jquery-ui.css", :media => "all" %>
<!--[if lt IE 8]><link rel="stylesheet" href="/assets/bootstrap-ie7patches.css"><![endif]-->
