<%- model_class = ApplyForTest -%>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:email) %></th>
      <th><%= model_class.human_attribute_name(:description) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @apply_for_tests.each do |apply_for_test| %>
    <tr>
      <td><%= link_to apply_for_test.id, cpanel_apply_for_test_path(apply_for_test) %></td>
      <td><%= apply_for_test.email %></td>
      <td><%= apply_for_test.name %>
      </td>
      <td><%= apply_for_test.created_at %></td>
      <td>
        <%= link_to t('.edit', :default => t("helpers.links.edit")),
        edit_cpanel_apply_for_test_path(apply_for_test), :class => 'btn btn-mini' %>
        <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
        cpanel_apply_for_test_path(apply_for_test),
        :method => :delete,
        :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
        :class => 'btn btn-mini btn-danger' %>
      </td>
    </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
new_cpanel_apply_for_test_path,
:class => 'btn btn-primary' %>

      <%= paginate @apply_for_tests  %>